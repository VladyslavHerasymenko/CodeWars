class FileMaster:
    
    def __init__(self, filePath):
        self.filePath = filePath
    
    def extension(self):
        return self.filePath[self.filePath.find(".") + 1:]
    
    def filename(self):
        return self.filePAth[self.findPath.rfind("/") + 1:self.findPath.find(".")]
    
    def dirpath(self)
        return self.filePath[:self.findPath.rfind("/") + 1]
